class Downvote is subclass of Vote
	operations
		/**
		*	Default Constructor
		*/
		public Downvote : nat*nat*nat ==> Downvote
		Downvote(type, uID, postID) == (
			userID := uID;
			cases type:
				0 -> questionID := postID,
				1 -> answerID := postID,
				2 -> commentID := postID
			end;
			return self;
		);
	functions
	traces
end Downvote